{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "baseUrl": "src/",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@dgi/*": [
        "./app/shared/dgi/*"
      ],
      "@env/*": [
        "./environments/*"
      ]
    },
    "lib": [
      "es2016",
      "dom"
    ]
  }
}